{
  "name": "parte-ui",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/parte-ui.es.js",
      "require": "./dist/parte-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/parte-ui.umd.js",
  "module": "./dist/parte-ui.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hooks": "^1.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubric-dev/parte-ui"
  },
  "keywords": [],
  "author": "rubric-dev",
  "bugs": {
    "url": "https://github.com/rubric-dev/parte-ui/issues"
  },
  "homepage": "https://github.com/rubric-dev/parte-ui#readme",
  "license": "MIT"
}
